{
  "name": "vue-coe-select",
  "version": "0.1.9",
  "private": true,
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-standard": "^3.0.3",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "bili": "^3.1.2",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.9.3",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-vue": "^4.3.1",
    "sinon": "^7.1.1",
    "vue-jest": "^3.0.0",
    "vue-template-compiler": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniazvd/vue-coe-select"
  },
  "license": "MIT",
  "scripts": {
    "build": "bili",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
